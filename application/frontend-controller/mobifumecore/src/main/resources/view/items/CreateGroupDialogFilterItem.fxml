<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>
<HBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/8.0.191"
      fx:controller="com.attestorforensics.mobifumecore.controller.item.CreateGroupDialogFilterItemController">
    <children>
        <ComboBox fx:id="filter" prefWidth="300.0" styleClass="dialogContent" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        <Button fx:id="errorButton" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" managed="false"
                mnemonicParsing="false" onAction="#onErrorInfo" prefHeight="45.6"
                style="-fx-background-color: transparent;" visible="false" AnchorPane.bottomAnchor="0.0"
                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <graphic>
                <ImageView fx:id="errorIcon" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true"/>
            </graphic>
        </Button>
        <HBox alignment="CENTER" prefWidth="161.6">
            <children>
                <Text fx:id="date" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="contentText"/>
            </children>
            <HBox.margin>
                <Insets left="8.0"/>
            </HBox.margin>
        </HBox>
    </children>
    <stylesheets>
        <URL value="@../style/Dialog.css"/>
        <URL value="@../style/DefaultApp.css"/>
        <URL value="@../style/Controls.css"/>
    </stylesheets>
</HBox>
